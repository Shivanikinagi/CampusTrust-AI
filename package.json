{
  "name": "campustrust-ai",
  "version": "1.0.0",
  "description": "CampusTrust AI - AI-Powered Decentralized Campus Governance on Algorand",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "dev:ai": "cd ai_engine && python app.py",
    "dev:backend": "cd backend && node server.js",
    "dev:full": "concurrently \"npm run dev:ai\" \"npm run dev\"",
    "dev:gasless": "concurrently \"npm run dev:backend\" \"npm run dev\"",
    "build": "vite build",
    "preview": "vite preview",
    "compile:contracts": "cd smart_contracts && python compile_contracts.py",
    "deploy:contracts": "cd smart_contracts && python deploy.py",
    "generate:sponsor": "cd backend && node generateSponsor.js"
  },
  "keywords": [
    "algorand",
    "ai",
    "campus-governance",
    "blockchain",
    "pyteal",
    "education-technology"
  ],
  "author": "CampusTrust AI Team",
  "license": "MIT",
  "devDependencies": {
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.22",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.19",
    "vite": "^7.2.7"
  },
  "dependencies": {
    "@perawallet/connect": "^1.5.0",
    "algosdk": "^3.5.2",
    "concurrently": "^8.2.2",
    "expo": "~54.0.33",
    "lucide-react": "^0.564.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-qr-code": "^2.0.18",
    "react-webcam": "^7.2.0",
    "recharts": "^2.12.7"
  }
}
